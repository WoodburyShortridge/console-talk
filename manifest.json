{
  "manifest_version": 2,
  "name": "Console Talk",
  "description": "This extension makes your console talk",
  "version": "0.0.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Console Talk",
    "default_popup": "popup.html"
  },
  "content_scripts": [ {
    "js": [ "inject.js" ],
    "matches": [ "*://*/*"],
    "run_at": "document_start"
  }],
  "web_accessible_resources": ["consoleTalk.js"],
  "permissions": [
    "tabs",
    "activeTab",
    "webRequest",
    "storage",
    "notifications",
    "*://*/"
  ]
}